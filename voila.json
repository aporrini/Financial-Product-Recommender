{
  "TagRemovePreprocessor": {
    "enabled": true,

    /* 1️⃣  NON eseguire le celle con questo tag               */
    "remove_cell_tags": ["remove_cell"],

    /* 2️⃣  Puoi lasciare vuoto: le celle rimosse non mostrano input */
    "remove_input_tags": [],

    /* 3️⃣  Continua a sopprimere l’output delle celle con quest’altro tag */
    "remove_output_tags": ["remove_output"]
  },

  "VoilaConfiguration": {
    /* nasconde il codice delle celle eseguite */
    "strip_sources": true,

    /* mostra il traceback in caso di eccezione */
    "show_tracebacks": true
  }
}
